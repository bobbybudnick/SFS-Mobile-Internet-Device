#setup kde
rm -r ~/.local/share/kscreen
cp -r kscreen ~/.local/share/kscreen/
cp -r applications ~/.local/share/applications
cp powermanagementprofilesrc ~/.config
cp plasma-org.kde.plasma.desktop-appletsrc ~/.config
cp kwinrulesrc ~/.config
cp kwinrc ~/.config
cp ksmserverrc ~/.config
cp kscreenlockerrc ~/.config
cp breezerc ~/.config
mkdir ~/.config/menus
cp applications-kmenuedit.menu ~/.config/menus/

#setup on screen keyboard
sudo cp keyboard.xml.STANDARD /usr/share/matchbox-keyboard/keyboard.xml

echo "All finished.  Reboot or login/logout once more and test the main thread with a USB keyboard attached.  If everything works, you can set it to autostart."
