#run this first
#run as root
#very important for stability on pc stick

#install kernel and probably overwrite some alsa files
sudo dpkg -i --force-all lattepanda-z8350-64-mainline_4.12.0-rc1-1_amd64.deb

#setup grub with intel power hack
sudo cp grub /etc/default/grub

#update grub within kernel configuration
sudo update-grub

echo "Please reboot immediately and run SETUP_PHASE_2"

